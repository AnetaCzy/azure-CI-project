{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adf-retail-aneta"
		},
		"LS_adls_stganeta_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'LS_adls_stganeta'"
		},
		"LS_sql_retailDB_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'LS_sql_retailDB'"
		},
		"LS_adls_stganeta_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://stgaccaneta.dfs.core.windows.net/"
		},
		"LS_sql_retailDB_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "sql-retail-server-aneta.database.windows.net"
		},
		"LS_sql_retailDB_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "retail_db"
		},
		"LS_sql_retailDB_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "rootadmin"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adf-retail-aneta_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"LS_adls_stganeta_accountKey": {
						"value": "[parameters('LS_adls_stganeta_accountKey')]"
					},
					"LS_sql_retailDB_password": {
						"value": "[parameters('LS_sql_retailDB_password')]"
					},
					"LS_adls_stganeta_properties_typeProperties_url": {
						"value": "[parameters('LS_adls_stganeta_properties_typeProperties_url')]"
					},
					"LS_sql_retailDB_properties_typeProperties_server": {
						"value": "[parameters('LS_sql_retailDB_properties_typeProperties_server')]"
					},
					"LS_sql_retailDB_properties_typeProperties_database": {
						"value": "[parameters('LS_sql_retailDB_properties_typeProperties_database')]"
					},
					"LS_sql_retailDB_properties_typeProperties_userName": {
						"value": "[parameters('LS_sql_retailDB_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}